# ----------------------------
# Node / Next.js (frontend)
# ----------------------------
node_modules/
.next/
.next/cache/
out/
dist/
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*
*.tsbuildinfo

# If you build static export
# out/

# ----------------------------
# Python (backend)
# ----------------------------
backend/.venv/
backend/__pycache__/
backend/**/*.py[cod]
*.pyc
*.pyo
*.pyd
.pytest_cache/
coverage/

# ----------------------------
# App runtime data (DON'T version trained data/models)
# ----------------------------
backend/data/dataset/
backend/data/model/
backend/data/*.db
backend/data/*.sqlite
backend/data/*.log

# keep empty folders with .gitkeep if you want the dirs present
!backend/data/.gitkeep
!backend/data/dataset/.gitkeep
!backend/data/model/.gitkeep

# ----------------------------
# Environment / secrets
# ----------------------------
.env
.env.*
backend/.env
backend/.env.*
frontend/.env
frontend/.env.*
frontend/.env.local

# If settings.json contains secrets, ignore it; otherwise you may commit it.
# Uncomment the next line if it has secrets:
# backend/settings.json

# ----------------------------
# Editor / OS noise
# ----------------------------
.vscode/
.idea/
*.code-workspace
.DS_Store
Thumbs.db
desktop.ini
~$*

